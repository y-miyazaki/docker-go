{
    "dockerFile": "../../Dockerfile",
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": ["esbenp.prettier-vscode", "ms-vscode.go"],
    "runArgs": [
      "-v",
      "{your application code directory}:{your application code docker directory}",
      "-v",
      "{your private key file}:/root/.ssh/id_rsa",
      "--env-file=.env",
      "--name",
      "app-local"
    ],
    "workspaceFolder": "{your application code directory}",
    "overrideCommand": false
}